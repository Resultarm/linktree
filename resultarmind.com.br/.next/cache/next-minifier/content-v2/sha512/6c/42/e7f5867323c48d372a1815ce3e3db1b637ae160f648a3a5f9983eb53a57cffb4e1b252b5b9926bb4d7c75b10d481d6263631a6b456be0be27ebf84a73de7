X = Math.abs(x0Px) + Math.abs(x1Px);\n  this.totalDistX = view.width + rect.width + totalAbsOffX;\n  var totalDistTrueX = view.width + rect.width + (x1Px > x0Px ? totalAbsOffX * -1 : totalAbsOffX); // const speed = totalDistTrueY / originTotalDistY;\n\n  var multiplierY = rect.originTotalDistY / totalDistTrueY;\n  var multiplierX = rect.originTotalDistX / totalDistTrueX;\n  this.top = rect.top;\n  this.bottom = rect.bottom;\n\n  if (y0Px < 0) {\n    this.top = this.top + y0Px * multiplierY;\n  }\n\n  if (y1Px > 0) {\n    this.bottom = this.bottom + y1Px * multiplierY;\n  }\n\n  this.left = rect.left;\n  this.right = rect.right;\n\n  if (x0Px < 0) {\n    this.left = this.left + x0Px * multiplierX;\n  }\n\n  if (x1Px > 0) {\n    this.right = this.right + x1Px * multiplierX;\n  }\n};\n\nvar _default = Bounds;\nexports.default = _default;\n\n/***/ })\n\n},[[\"vlRD\",1,2,0,3,4,5,6]]]);","inputSourceMap":{"version":3,"sources":["/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/node_modules/react-scroll-parallax/cjs/index.js","/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/node_modules/react-scroll-parallax/cjs/components/withController.js","/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/node_modules/react-scroll-parallax/cjs/components/Parallax.js","/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/node_modules/react-wow/dist/index.js","/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/node_modules/@material-ui/icons/SupervisorAccount.js","/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/node_modules/youtube-player/dist/YouTubePlayer.js","/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/node_modules/url-loader/dist/cjs.js??ref--6-0!/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/public/images/saas/deco-wave-light.png","/Users/franciscosantos/go/src/Works/rmcsm/resultarmind.com.br/node_modules/youtube-player/dist/FunctionStateMap.js","/Users/franciscosantos/go/src/Wor